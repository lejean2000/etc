59,109&nbsp;&nbsp; 60,580&nbsp;&nbsp; 63,9&nbsp;&nbsp;
69,63&nbsp;&nbsp; 71,846&nbsp;&nbsp; 72,191&nbsp;&nbsp;
73,1046&nbsp;&nbsp; 77,57&nbsp;&nbsp; 78,40&nbsp;&nbsp;
81,579&nbsp;&nbsp; <b>90,90</b>&nbsp;&nbsp; 95,92&nbsp;&nbsp;
96,91&nbsp;&nbsp; 97,48&nbsp;&nbsp; 99,1577&nbsp;&nbsp;
110,656&nbsp;&nbsp; 219,165&nbsp;&nbsp; 228,1045&nbsp;&nbsp;
248,1580&nbsp;&nbsp; 249,163&nbsp;&nbsp; 250,162&nbsp;&nbsp;
252,564&nbsp;&nbsp; 254,921&nbsp;&nbsp; 271,84&nbsp;&nbsp;
283,3&nbsp;&nbsp; 348,169&nbsp;&nbsp; 394,610&nbsp;&nbsp;
603,978&nbsp;&nbsp; 648,822&nbsp;&nbsp; 651,652&nbsp;&nbsp;
