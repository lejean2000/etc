    <tr> <td>incircle</td> <td>X(1)</td> <td>r</td> </tr>
    <tr> <td>nine-point circle</td> <td>X(5)</td> <td>R/2</td>     </tr>
    <tr> <td>Brocard circle</td> <td>X(182)</td> <td>eR/(2 cos &omega;)</td>     </tr>
    <tr> <td>1st Lemoine circle</td> <td>X(182)</td> <td>(1/2)(R sec &omega;)</td>     </tr>
    <tr> <td>2nd Lemoine circle (cosine circle)</td> <td>X(6)</td> <td>abc/(a<sup>2</sup> + b<sup>2</sup> + c<sup>2</sup>)</td>     </tr>
    <tr> <td>Spieker circle</td> <td>X(10)</td> <td>r/2</td>     </tr>
    <tr> <td>Apollonius circle</td> <td>X(970)</td> <td>(r<sup>2</sup> + s<sup>2</sup>)/4r</td> </tr>
    <tr> <td>Bevan circle</td> <td>X(40)</td> <td>2R</td>     </tr>
</table>
